#include<studio.h> 
void main()
{ 
int a,b,c,t;
Printf("Enter the marks : ");
scanf("%d%d%d",&a,&b,&c);
t=(a+b+c)/3;
if(t<=100 && t>=85)
{ 
printf("Grade A");
}
else if(t<=84 && t>=70)
{
printf("Grade B");
}
else if(t<=69 && t>=55)
{
printf("Grade C")
}
else if(t<=54 && t>=40)
{ 
printf ("Grade D);
}
else
{ 
printf ("Grade F");
}
}
