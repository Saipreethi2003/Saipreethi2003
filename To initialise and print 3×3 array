#include<stdio.h>
void main()
{
int a[3][3],i,j,sum;
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf ("element-[%d],[%d]:",i,j);
scanf("%d",&a[i][j]);
}
}
printf("The matrix is:\n");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf ("%d\t",a[i][j]);
}
printf ("\n");
}
sum=a[0][0]+a[1][1]+a[2][2];
printf ("Sum of diagonal elements is %d",sum);
}
